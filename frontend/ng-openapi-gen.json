{
  "$schema": "node_modules/ng-openapi-gen/ng-openapi-gen-schema.json",
  "input": "http://localhost:8080/api-docs",
  "output": "src/app/core/api",
  "servicePrefix": "",
  "serviceSuffix": "Client",
  "modelSuffix": "",
  "enumStyle": "alias",
  "templates": null,
  "excludeTags": [],
  "includeTags": [],
  "ignoreUnusedModels": false,
  "removeStaleFiles": true,
  "indexFile": true,
  "customizedResponseType": {
    "toJson": {
      "operationIds": ["login", "register", "refresh", "logout", "listar", "buscar", "criar", "atualizar", "excluir", "marcarConcluido", "reabrir"],
      "accept": "application/json"
    }
  }
}
